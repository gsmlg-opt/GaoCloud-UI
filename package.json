{
  "name": "GaoCloud-UI",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "node packages/ui/server",
    "prebuild": "npm run build:clean",
    "build": "NODE_ENV=production webpack --config packages/ui/internals/webpack/webpack.prod.babel.js --color --progress",
    "build:clean": "rimraf ./build",
    "generate": "plop --plopfile generators/index.js",
    "test": "bun run test",
    "lint": "bun run lint:ui && bun run lint:utils",
    "lint:staged": "bun run lint:staged",
    "lint:ui": "cd packages/ui && bun run lint",
    "lint:utils": "cd packages/utils && bun run lint"
  },
  "pre-commit": "lint:staged",
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "plop": "^4.0.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {},
  "overrides": {
    "cheerio": "<1.0.0-rc.3"
  }
}
