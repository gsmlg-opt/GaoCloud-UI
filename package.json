{
  "name": "GaoCloud-UI",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node packages/ui/server",
    "prebuild": "npm run build:clean",
    "build": "NODE_ENV=production webpack --config packages/ui/internals/webpack/webpack.prod.babel.js --color --progress",
    "build:clean": "rimraf ./build",
    "generate": "plop --plopfile generators/index.js",
    "test": "bun run test"
  },
  "devDependencies": {
    "plop": "^4.0.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {},
  "overrides": {
    "cheerio": "<1.0.0-rc.3"
  }
}
