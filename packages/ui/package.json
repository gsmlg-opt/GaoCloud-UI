{
  "name": "@GaoCloud-UI/ui",
  "version": "2.0.0",
  "description": "GaoCloud manage system ui",
  "repository": {
    "type": "git",
    "url": "git://github.com/gsmlg-opt/GaoCloud-UI.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.10.0"
  },
  "author": "GSMLG",
  "license": "MIT",
  "files": [
    "build"
  ],
  "scripts": {
    "analyze:clean": "rm -f stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "optimize-svg": "svgo --enable=removeStyleElement,removeAttrs --config='{\"plugins\": [{\"removeAttrs\": {\"attrs\": \"(id|class)\"} }] }' --indent=2 --pretty -r -f app/images",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run build:clean",
    "build": "NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color --progress",
    "build:clean": "rimraf ./build",
    "start": "NODE_ENV=development node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "NODE_ENV=production node server",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --ignore-path .gitignore --ignore-pattern internals/scripts .",
    "lint:css": "stylelint './app/**/*.js'",
    "lint:eslint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean",
    "test:clean": "rm -rf ./coverage",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "7.0.0",
    "@fortawesome/free-solid-svg-icons": "7.0.0",
    "@fortawesome/react-fontawesome": "3.0.0",
