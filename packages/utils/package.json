{
  "name": "@gsmlg/utils",
  "private": false,
  "author": "GSMLG",
  "version": "2.0.0",
  "description": "Functions of SingleCloud.",
  "keywords": [
    "singlecloud"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn build:es",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./src --out-dir ./build --ignore \"**/*.test.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ../../babel.config.js ./src --out-dir ./build/esm --ignore \"**/*.test.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ../../babel.config.js ./src --out-dir ./build/es --ignore \"**/*.test.js\"",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "prebuild": "rimraf build",
    "release": "yarn build && npm publish build --tag next",
    "lint": "npm run lint:js",
    "lint:eslint": "ESLINT_USE_FLAT_CONFIG=false eslint",
    "lint:eslint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint --fix -- src",
    "lint:js": "npm run lint:eslint:fix -- src ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rm -rf ./coverage",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "typescript": "tslint -p tsconfig.json \"{src,test}/**/*.{ts,tsx}\""
  },
  "main": "./src/index.js",
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ]
  },
  "peerDependencies": {},
  "dependencies": {
    "crypto-js": "^4.2.0",
    "warning": "^4.0.3"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-object-assign": "7.27.1",
    "@babel/plugin-transform-react-constant-elements": "7.27.1",
    "@babel/plugin-transform-runtime": "7.28.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "coveralls": "3.1.1",
    "@babel/eslint-parser": "^7.24.7",
    "eslint": "9.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.32.0",
    "jest-cli": "30.0.5"
  }
}
