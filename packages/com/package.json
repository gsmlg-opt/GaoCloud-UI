{
  "name": "@gsmlg/com",
  "private": false,
  "author": "GSMLG",
  "version": "2.0.0",
  "type": "module",
  "description": "React components of SingleCloud.",
  "keywords": [
    "singlecloud"
  ],
  "license": "MIT",
  "scripts": {
    "build": "bun build:cjs && bun build:esm && bun build:es",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./src --out-dir ./build --ignore \"**/*.test.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ../../babel.config.js ./src --out-dir ./build/esm --ignore \"**/*.test.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ../../babel.config.js ./src --out-dir ./build/es --ignore \"**/*.test.js\"",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "prebuild": "rimraf build",
    "release": "bun build && npm publish build --tag next",
    "typescript": "tslint -p tsconfig.json \"{src,test}/**/*.{ts,tsx}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "main": "./src/index.js",
  "peerDependencies": {},
  "dependencies": {
    "@mui/system": "^5.16.7",
    "@mui/utils": "^5.16.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.19",
    "warning": "^4.0.3"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "7.28.3",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-object-assign": "7.27.1",
    "@babel/plugin-transform-react-constant-elements": "7.27.1",
    "@babel/plugin-transform-runtime": "7.28.3",
    "@data-ui/network": "0.0.84",
    "@storybook/addon-actions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/react": "^8.3.0",
    "babel-loader": "10.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-dev-warning": "^0.1.1"
  }
}
